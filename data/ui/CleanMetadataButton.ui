<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk+" version="3.24"/>
  <requires lib="libhandy" version="1.0"/>
  <template class="CleanMetadataButton" parent="GtkBin">
    <child>
      <object class="GtkButton" id="_button">
        <property name="label" translatable="yes">_Clean metadata</property>
        <property name="visible">True</property>
        <property name="can-focus">True</property>
        <property name="receives-default">True</property>
        <property name="use-underline">True</property>
        <style>
          <class name="destructive-action"/>
        </style>
        <signal name="clicked" handler="_on_clean_metadata_button_clicked" swapped="no"/>
      </object>
    </child>
  </template>
</interface>
